{
  "name": "sms-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "sequelize-cli db:migrate --migrations-path src/migrations --config src/config/config.cjs",
